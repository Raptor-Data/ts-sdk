{
    "name": "@raptor-data/ts-sdk",
    "version": "1.0.1",
    "description": "Official TypeScript SDK for RaptorData API",
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./hooks": {
            "import": {
                "types": "./dist/hooks.d.ts",
                "default": "./dist/hooks.js"
            },
            "require": {
                "types": "./dist/hooks.d.cts",
                "default": "./dist/hooks.cjs"
            }
        },
        "./examples": {
            "import": {
                "types": "./dist/examples.d.ts",
                "default": "./dist/examples.js"
            },
            "require": {
                "types": "./dist/examples.d.cts",
                "default": "./dist/examples.cjs"
            }
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "format": "prettier --write \"src/**/*.{ts,json}\"",
        "format:check": "prettier --check \"src/**/*.{ts,json}\"",
        "check": "npm run typecheck && npm run lint && npm run format:check && npm run test",
        "prepublishOnly": "pnpm run build"
    },
    "keywords": [
        "raptor-data",
        "raptor",
        "data",
        "rag",
        "documents",
        "pdf",
        "llm",
        "ai",
        "api",
        "sdk",
        "raptor-data-api",
        "raptor-data-sdk",
        "raptor-data-ts-sdk",
        "typescript",
        "raptor-data",
        "document-processing",
        "embeddings",
        "vector-search"
    ],
    "author": "Raptor Data <info@raptordata.dev>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/raptor-data/ts-sdk.git"
    },
    "bugs": {
        "url": "https://github.com/raptor-data/ts-sdk/issues"
    },
    "homepage": "https://github.com/raptor-data/ts-sdk#readme",
    "engines": {
        "node": ">=18.0.0"
    },
    "peerDependencies": {
        "react": ">=16.8.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        }
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/react": "^18.0.0",
        "eslint": "^9.38.0",
        "eslint-config-prettier": "^10.1.8",
        "prettier": "^3.6.2",
        "tsup": "^8.3.0",
        "typescript": "^5.6.0",
        "typescript-eslint": "^8.46.2",
        "jest": "^29.0.0"
    }
}
